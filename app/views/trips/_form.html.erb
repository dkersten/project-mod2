<%= form_for @trip do |f| %>

  <%= f.hidden_field :user_id, :value => @current_user.id %>

  <%= f.label :destination_id %>
  <%= f.collection_select :destination_id, @destinations, :id, :city_country %>
  <%= f.label :flight_class_id %>
  <%= f.collection_select :flight_class_id, @flights, :id, :name %>
  <%= f.label :accommodation_id %>
  <%= f.collection_select :accommodation_id, @accommodations, :id, :name %>
  <%= f.submit %>
<% end %>